{
    "name": "jefhar/laravel-export-constants",
    "description": "Export your PHP class constants to JavaScript",
    "keywords": [
        "jefhar",
        "laravel-export-constants"
    ],
    "homepage": "https://github.com/jefhar/laravel-export-constants",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jeff Harris",
            "email": "jeff@jeffharris.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "haydenpierce/class-finder": "^0.4.3",
        "illuminate/support": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelExportConstants\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelExportConstants\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelExportConstants\\LaravelExportConstantsServiceProvider"
            ],
            "aliases": {
            }
        }
    }
}
